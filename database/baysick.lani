 object SquareRoot extends Baysick {
   def main(args:Array[String]) = {
     10 PRINT "Enter a number"
     20 INPUT 'n
     30 PRINT "Square root of " % "'n is " % SQRT('n)
     40 END
     RUN
   }
 }